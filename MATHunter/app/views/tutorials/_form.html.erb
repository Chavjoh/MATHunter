<%= form_for @tutorial do |f| %>
	Title: <%= f.text_field :title %><br />
	Difficulty: <%= f.select :difficulty, options_for_select([["Easy", "1"], ["Medium", "2"], ["Hard", "3"]]) %><br />
	Description: <%= f.text_area :description %><br />
	Image: <%= f.text_field :image %><br />
	<%= f.submit %>
<% end %>